<form id="signup" action="submit">
    <div class="card">
        <div class="card-header">
            <h2>Sign Up!</h2>
        </div>
        <div class="card-body">
            <label for="email-signup">Email:</label>
            <input id="email" type="email"/>
            <br/>
            <label for="username-sign">Username:</label>
            <input id="username" type="text"/>
            <br/>
            <label for="password-signup">Password:</label>
            <input id="password" type="password"/>
            <br/>
            <label for="password-confirm-signup">Confirm Password:</label>
            <input id="confirm-password" type="password">
            <br/>
            <div class="avatar-img-cont">
                <img src="/images/allsports.png" alt="all sports" data-url="/images/allsports.png">
                <img style="width: 10%" src="/images/tv.jpg" alt="TV" data-url="/images/tv.jpg">
            </div>
            <br/>
            <button type="submit">Sign up</button>
        </div>
    </div>
</form>

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous"></script>
<script src="/js/signup.js"></script>